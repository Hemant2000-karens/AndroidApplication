<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/home3"
        android:contentDescription="@string/todo" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-20dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/home1"
        android:contentDescription="@string/todo" />

    <androidx.cardview.widget.CardView
        android:layout_width="374dp"
        android:layout_height="188dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="17dp"
        app:cardBackgroundColor="#5E6501"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp">

        <TextView
            android:id="@+id/usr_name"
            android:layout_width="222dp"
            android:layout_height="69dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/username_field"
            android:fontFamily="@font/adamina"
            android:gravity="center"
            android:textAlignment="center"
            android:textColor="#001E77"
            android:textSize="18sp" />

        <Button
            android:id="@+id/logout_home"
            android:layout_width="124dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="200dp"
            android:layout_marginTop="115dp"
            android:background="@drawable/logout2"
            android:text="@string/logout"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />
    </androidx.cardview.widget.CardView>

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="490dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp">

        <LinearLayout
            android:id="@+id/linaer"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center">

            <androidx.gridlayout.widget.GridLayout
                android:id="@+id/gr1"
                android:layout_width="410dp"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                app:columnCount="2"
                app:rowCount="2"
                app:useDefaultMargins="true">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <ImageButton
                        android:id="@+id/add_buttin"
                        android:layout_width="102dp"
                        android:layout_height="108dp"
                        android:layout_marginStart="23dp"
                        android:background="@drawable/icons8_add_database_50"
                        android:contentDescription="@string/button" />

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:text="@string/add_records"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="24sp" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <ImageButton
                        android:id="@+id/issue_but"
                        android:layout_width="102dp"
                        android:layout_height="108dp"
                        android:layout_marginStart="23dp"
                        android:contentDescription="@string/todo"
                        app:srcCompat="@drawable/icons8_multiple_devices_50" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/issue_mutli_dev"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <ImageButton
                        android:id="@+id/imageButton3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/replace_multi_dev"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="18sp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <ImageButton
                        android:id="@+id/m_isrep"
                        android:layout_width="102dp"
                        android:layout_height="108dp"
                        android:layout_marginStart="23dp"
                        android:contentDescription="@string/button"
                        app:srcCompat="@drawable/icons8_iphone_x_50" />

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/mobile"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>

            </androidx.gridlayout.widget.GridLayout>


            <androidx.gridlayout.widget.GridLayout
                android:id="@+id/gr2"
                android:layout_width="410dp"
                android:layout_height="match_parent"
                android:layout_marginStart="30dp"
                app:columnCount="2"
                app:rowCount="2"
                app:useDefaultMargins="true"
                tools:layout_marginStart="10dp">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/issue_mutli_dev"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/issue_mutli_dev"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/issue_mutli_dev"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginTop="50dp"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp"
                    app:cardPreventCornerOverlap="true"
                    app:cardUseCompatPadding="false"
                    app:layout_gravity="center">

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="100dp"
                        android:gravity="center"
                        android:text="@string/issue_mutli_dev"
                        android:textAlignment="center"
                        android:textColor="#CDCA05"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>
            </androidx.gridlayout.widget.GridLayout>
