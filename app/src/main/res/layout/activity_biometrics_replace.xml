<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Biometrics_replace">

    <view
        android:id="@+id/view17"
        class="androidx.viewpager.widget.ViewPager"
        android:layout_width="411dp"
        android:layout_height="731dp"
        android:background="@drawable/art_purple"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="412dp"
        android:layout_height="53dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="678dp"
        android:background="#9E6A5800"
        android:fontFamily="@font/taviraj_medium"
        android:gravity="center"
        android:text="@string/biometrics_replace"
        android:textAlignment="center"
        android:textColor="#F6EEEE"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view17"
        app:layout_constraintEnd_toEndOf="@+id/view17"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="@+id/view17" />

    <ImageButton
        android:id="@+id/Bio_back"
        android:layout_width="52dp"
        android:layout_height="53dp"
        android:layout_marginEnd="359dp"
        android:background="@drawable/vannila"
        android:contentDescription="@string/todo"
        app:layout_constraintEnd_toEndOf="@+id/textView22"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView22"
        app:layout_constraintTop_toTopOf="@+id/textView22"
        app:srcCompat="@drawable/homw" />

    <view
        android:id="@+id/view18"
        class="androidx.viewpager.widget.ViewPager"
        android:layout_width="348dp"
        android:layout_height="622dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="83dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="26dp"
        android:background="#2DFFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view17"
        app:layout_constraintEnd_toEndOf="@+id/view17"
        app:layout_constraintStart_toStartOf="@+id/view17"
        app:layout_constraintTop_toTopOf="@+id/view17" />
</androidx.constraintlayout.widget.ConstraintLayout>